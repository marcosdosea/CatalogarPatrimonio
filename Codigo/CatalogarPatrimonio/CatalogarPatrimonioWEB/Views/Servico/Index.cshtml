@model IEnumerable<CatalogarPatrimonioWEB.Models.ServicoModel>

@{ 
    ViewData["Title"] = "Index";
    Layout = "_LayoutInside";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataSolicitacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataAutorizacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataConclusao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DataSolicitacao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DataAutorizacao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DataConclusao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descricao)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new {  id = item.Id}) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id}) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id})
                </td>
            </tr>
        }
    </tbody>
</table>