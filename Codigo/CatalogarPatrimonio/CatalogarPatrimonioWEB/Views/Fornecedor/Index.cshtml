@model IEnumerable<CatalogarPatrimonioWEB.Models.FornecedorModel>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutInside";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id = item.IdFornecedor }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdFornecedor }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdFornecedor })
            </td>
        </tr>
}
    </tbody>
</table>
